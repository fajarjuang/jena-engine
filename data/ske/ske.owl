    <publicationBibtex>Pair programming</publicationBibtex>
    <publicationBibtex>Peer review</publicationBibtex>
    <publicationBibtex>Software process model</publicationBibtex>
    <publicationBibtex>Software verification},
  document_type = {Conference Paper},
  journal = {ISESE'06 - Proceedings of the 5th ACM-IEEE International Symposium
 on Empirical Software Engineering},
  owner = {Marcos},
